build: Main.o HashMap.o PreProcesare.o DefineIf.o Garzi.o Include.o
	gcc -std=c89 -o so-cpp Main.o HashMap.o PreProcesare.o DefineIf.o Garzi.o Include.o

Include.o: Include.c
	gcc -c Include.c

Garzi.o: Garzi.c
	gcc -c Garzi.c

DefineIf.o: DefineIf.c
	gcc -c DefineIf.c

Main.o: Main.c
	gcc -c Main.c

HashMap.o: HashMap.c
	gcc -c HashMap.c

PreProcesare.o: PreProcesare.c
	gcc -c PreProcesare.c