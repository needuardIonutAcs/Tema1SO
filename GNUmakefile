build: Main.o HashMap.o PreProcesare.o DefineIf.o Garzi.o Include.o
	gcc -Wall -g -Wextra -std=c89 -o so-cpp Main.o HashMap.o PreProcesare.o DefineIf.o Garzi.o Include.o

Include.o: Include.c
	gcc -Wall -g -Wextra -c Include.c

Garzi.o: Garzi.c
	gcc -Wall -g -Wextra -c Garzi.c

DefineIf.o: DefineIf.c
	gcc -Wall -g -Wextra -c DefineIf.c

Main.o: Main.c
	gcc -Wall -g -Wextra -c Main.c

HashMap.o: HashMap.c
	gcc -Wall -g -Wextra -c HashMap.c

